<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_branch_tree" model="ir.ui.view">
        <field name="name">branch.tree.view</field>
        <field name="model">employee.branch</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="view_branch_form" model="ir.ui.view">
        <field name="name">branch.form.view</field>
        <field name="model">employee.branch</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_branch_view" model="ir.actions.act_window">
        <field name="name">Employee Branch</field>
        <field name="res_model">employee.branch</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>